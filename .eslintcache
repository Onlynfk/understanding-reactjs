wordIsValid, setPasswordIsValid] = useState();\n  const [formIsValid, setFormIsValid] = useState(false);\n\n\n\n  // using UseReducer\n  const [emailState, dispatchEmail] = useReducer(useEmailReducer, {\n    value : '',\n    isValid : null\n  });\n\n  const [passwordState, dispatchPassword] = useReducer(usePaswordReducer,{\n    value : '',\n    isValid : null\n  })\n\n  // ​object destructing \n  const {isValid: emailIsvalid} = emailState\n  const {isValid : passwordIsvalid} = passwordState\n\n  useEffect(() =>{\n\n   const identifier = setTimeout(() =>{\n    console.log('Checking validity')\n    setFormIsValid(\n      emailIsvalid && passwordIsvalid);\n   }, 200)\n\n   return() =>{\n     console.log('Clean Up')\n     clearTimeout(identifier);\n   }\n    \n\n  }, [emailIsvalid, passwordIsvalid ])\n\n  const emailChangeHandler = (event) => {\n    dispatchEmail({type: 'USER_INPUT', val:event.target.value})\n    \n    \n  };\n\n  const passwordChangeHandler = (event) => {\n    dispatchPassword({type: 'USER_INPUT', val:event.target.value})\n  };\n\n  const validateEmailHandler = () => {\n    dispatchEmail({type: 'INPUT_BLUR'})\n  };\n\n  const validatePasswordHandler = () => {\n    dispatchPassword({type: 'INPUT_BLUR'})\n\n  };\n\n  const submitHandler = (event) => {\n    event.preventDefault();\n    props.onLogin(emailState.value, passwordState.value);\n  };\n\n  return (\n    <Card className={classes.login}>\n      <form onSubmit={submitHandler}>\n        <div\n          className={`${classes.control} ${\n            emailState.isValid === false ? classes.invalid : ''\n          }`}\n        >\n          <label htmlFor=\"email\">E-Mail</label>\n          <input\n            type=\"email\"\n            id=\"email\"\n            value={emailState.value}\n            onChange={emailChangeHandler}\n            onBlur={validateEmailHandler}\n          />\n        </div>\n        <div\n          className={`${classes.control} ${\n            passwordState.isValid === false ? classes.invalid : ''\n          }`}\n        >\n          <label htmlFor=\"password\">Password</label>\n          <input\n            type=\"password\"\n            id=\"password\"\n            value={passwordState.value}\n            onChange={passwordChangeHandler}\n            onBlur={validatePasswordHandler}\n          />\n        </div>\n        <div className={classes.actions}>\n          <Button type=\"submit\" className={classes.btn} disabled={!formIsValid}>\n            Login\n          </Button>\n        </div>\n      </form>\n    </Card>\n  );\n};\n\nexport default Login;\n","F:\\projects\\works\\src\\components\\MainHeader\\Navigation.js",[],"F:\\projects\\works\\src\\components\\UI\\Card\\Card.js",[],"F:\\projects\\works\\src\\components\\UI\\Button\\Button.js",[],"F:\\projects\\works\\src\\components\\store\\auth-context.js",[],{"ruleId":"46","replacedBy":"47"},{"ruleId":"48","replacedBy":"49"},{"ruleId":"50","severity":1,"message":"51","line":10,"column":18,"nodeType":"52","messageId":"53","endLine":10,"endColumn":20},{"ruleId":"50","severity":1,"message":"51","line":19,"column":18,"nodeType":"52","messageId":"53","endLine":19,"endColumn":20},{"ruleId":"50","severity":1,"message":"51","line":35,"column":18,"nodeType":"52","messageId":"53","endLine":35,"endColumn":20},{"ruleId":"50","severity":1,"message":"51","line":46,"column":18,"nodeType":"52","messageId":"53","endLine":46,"endColumn":20},"no-native-reassign",["54"],"no-negated-in-lhs",["55"],"eqeqeq","Expected '===' and instead saw '=='.","BinaryExpression","unexpected","no-global-assign","no-unsafe-negation"]